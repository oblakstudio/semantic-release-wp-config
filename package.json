{
  "name": "semantic-release-wp-config",
  "version": "1.0.0",
  "license": "MIT",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/oblakstudio/semantic-release-wp-config.git"
  },
  "bugs": "https://githu.com/your_org/semantic-release-wp-config/issues",
  "keywords": [],
  "author": {
    "name": "Oblak Studio",
    "email": "support@oblak.studio"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc --watch",
    "build:production": "rimraf dist && tsc --project tsconfig.build.json",
    "lint": "eslint lib/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "release:test": "node ./node_modules/semantic-release/bin/semantic-release.js --dry-run",
    "release": "node ./node_modules/semantic-release/bin/semantic-release.js"
  },
  "engines": {
    "node": ">= 18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "semantic-release-wp-plugin": "^1.0"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "semantic-release": "^21"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
